<AnchorPane id="AnchorPane" prefHeight="770.0" prefWidth="1292.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Sportif.ListeCoachsDisponibilitesController">
   <children>
      <ImageView fitHeight="786.0" fitWidth="1299.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Aziz/Capture.PNG" />
         </image>
      </ImageView>
      <Button fx:id="retour" layoutX="278.0" layoutY="185.0" mnemonicParsing="false" onAction="#retour" style="-fx-background-radius: 60;" text="Retour">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <ColorAdjust contrast="-0.07" hue="0.7" saturation="0.15" />
         </effect></Button>
      <Button fx:id="demandereservation" disable="true" layoutX="811.0" layoutY="185.0" mnemonicParsing="false" onAction="#demandeReservation" style="-fx-background-radius: 60;" text="Demande Reservation">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <ColorAdjust contrast="-0.07" hue="0.7" saturation="0.15" />
         </effect></Button>
      <TableView fx:id="table" layoutX="354.0" layoutY="318.0" prefHeight="298.0" prefWidth="570.0" style="-fx-background-radius: 25;">
        <columns>
          <TableColumn fx:id="nomprenom" prefWidth="167.0" text="Coach" />
          <TableColumn fx:id="date" prefWidth="211.0" text="Date" />
            <TableColumn fx:id="heure" prefWidth="191.0" text="Heure" />
        </columns>
         <effect>
            <ColorAdjust hue="0.32" saturation="0.2" />
         </effect>
      </TableView>
      <TextField fx:id="recherche" layoutX="455.0" layoutY="185.0" prefHeight="39.0" prefWidth="304.0" style="-fx-background-radius: 60;">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <ColorAdjust contrast="-0.07" hue="0.7" saturation="0.15" />
         </effect></TextField>
      <RadioButton fx:id="pnom" layoutX="396.0" layoutY="250.0" mnemonicParsing="false" onAction="#choice" text="Par Nom">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></RadioButton>
      <RadioButton fx:id="pdate" layoutX="549.0" layoutY="250.0" mnemonicParsing="false" onAction="#choice" text="Par Date">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></RadioButton>
      <RadioButton fx:id="pheure" layoutX="694.0" layoutY="250.0" mnemonicParsing="false" onAction="#choice" text="Par Heure">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></RadioButton>
      <ImageView fitHeight="148.0" fitWidth="314.0" layoutX="954.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Aziz/logo%20fitnes.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
